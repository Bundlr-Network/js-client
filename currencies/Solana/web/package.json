{
    "name": "@bundlr-network/solana-web",
    "version": "0.0.1",
    "description": "Solana Currency for Bundlr",
    "main": "index.js",
    "author": "JesseTheRobot",
    "license": "MIT",
    "scripts": {
        "build": "rm -rf ./cjs && rm -rf ./esm && tsc -p ./cjs.json && tsc -p ./esm.json"
    },
    "exports": {
        ".": {
            "import": "./esm/web/index.js",
            "require": "./cjs/web/index.js"
        },
        "./*": {
            "import": "./*.js",
            "require": "./*.js"
        }
    },
    "files": [
        "cjs/**/*",
        "esm/**/*"
    ],
    "peerDependencies": {
        "@bundlr-network/client": "^0.6.2"
    },
    "dependencies": {
        "@noble/ed25519": "^1.6.0",
        "@solana/wallet-adapter-base": "^0.9.5",
        "@solana/web3.js": "^1.37.0",
        "async-retry": "^1.3.3",
        "base64url": "^3.0.1",
        "bignumber.js": "^9.0.2",
        "bs58": "^5.0.0"
    },
    "devDependencies": {
        "@bundlr-network/client": "^0.6.2",
        "@noble/ed25519": "^1.6.0",
        "@solana/wallet-adapter-base": "^0.9.5",
        "@solana/web3.js": "^1.37.0",
        "async-retry": "^1.3.3",
        "base64url": "^3.0.1",
        "bignumber.js": "^9.0.2",
        "bs58": "^5.0.0"
    }
}