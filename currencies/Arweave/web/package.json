{
    "name": "@bundlr-network/arweave-web",
    "version": "0.0.1",
    "description": "Arweave Currency for Bundlr",
    "main": "index.js",
    "author": "JesseTheRobot",
    "license": "MIT",
    "scripts": {
        "build": "rm -rf ./cjs && rm -rf ./esm && tsc -p ./cjs.json && tsc -p ./esm.json"
