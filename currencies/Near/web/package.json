        "esm/**/*"
    ],
    "peerDependencies": {
        "@bundlr-network/client": "^0.6.2"
    },
    "devDependencies": {
        "@bundlr-network/client": "^0.6.2",
        "bignumber.js": "^9.0.2",
        "bn.js": "^5.2.0",
        "bs58": "^5.0.0",
        "js-sha256": "^0.9.0",
        "near-api-js": "^0.44.2"
    }
}