            "require": "./cjs/web/index.js"
        }
    },
    "files": [
        "cjs/**/*",
        "esm/**/*"
    ],
    "peerDependencies": {
        "@bundlr-network/client": "^0.6.2"
    },
    "devDependencies": {
        "@bundlr-network/client": "^0.6.2"
    }
}