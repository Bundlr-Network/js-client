{
  "name": "@bundlr-network/client",
  "version": "0.6.2-rc1",
  "description": "JS client for Bundlr nodes",
  "repository": "https://github.com/Bundlr-Network/js-client.git",
  "author": "Bundlr Network",
  "license": "Apache-2.0",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "browser": "./build/esm/web/index.js",
  "files": [
    "build/**/*.js",
    "build/**/*.js.map",
    "build/**/*.ts"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    },
    "./web": {
      "import": "./build/esm/web/index.js",
      "require": "./build/cjs/web/index.js"
    },
    "./node": {
      "import": "./build/esm/node/index.js",
      "require": "./build/cjs/node/index.js"
    },
    "./signing": {
      "import": "./build/esm/common/signing",
      "require": "./build/cjs/common/signing"
    }
  },
  "scripts": {
    "dev:build": "rm -rf build && tsc -p cjs.tsconfig.json && tsc -p esm.tsconfig.json && chmod +x ./build/cjs/node/cli.js && chmod +x ./build/esm/node/cli.js && ./scripts/fixup.sh",
    "prod:build": "yarn run dev:build && webpack",
    "lint:check": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "lerna:build": "yarn run prod:build && lerna run build --concurrency 8"
  },
  "bin": {
    "bundlr": "./build/cjs/node/cli.js"
  },
  "dependencies": {
    "@supercharge/promise-pool": "^2.1.0",
    "arbundles": "^0.6.17-rc1",
    "arweave": "1.10.18",
    "async-retry": "^1.3.3",
    "base64url": "^3.0.1",
    "bignumber.js": "^9.0.1",
    "bs58": "^4.0.1",
    "commander": "^8.2.0",
    "csv": "^6.0.5",
    "inquirer": "^8.2.0",
    "js-sha256": "^0.9.0",
    "mime-types": "^2.1.34"
  },
  "devDependencies": {
    "@solana/wallet-adapter-base": "^0.9.2",
    "@solana/web3.js": "^1.36.0",
    "@types/async-retry": "^1.4.3",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.0",
    "@types/secp256k1": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "algosdk": "^1.13.1",
    "assert": "^2.0.0",
    "axios": "^0.25.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "ethers": "^5.5.1",
    "jest": "^27.2.4",
    "lerna": "^4.0.0",
    "longjohn": "^0.2.12",
    "near-api-js": "^0.44.2",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "serve": "^13.0.2",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "v8-profiler-next": "^1.5.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-base": "^0.9.2",
    "@solana/web3.js": "^1.36.0",
    "algosdk": "^1.13.1",
    "axios": "^0.25.0",
    "ethers": "^5.5.1",
    "near-api-js": "^0.44.2"
  },
  "peerDependenciesMeta": {
    "@solana/wallet-adapter-base": {
      "optional": true
    },
    "@solana/web3.js": {
      "optional": true
    },
    "near-api-js": {
      "optional": true
    },
    "algosdk": {
      "optional": true
    },
    "ethers": {
      "optional": true
    }
  }
}