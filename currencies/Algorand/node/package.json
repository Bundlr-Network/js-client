{
  "name": "@bundlr-network/algorand",
  "version": "0.0.1",
  "description": "Algorand Currency for Bundlr",
  "main": "index.js",
  "author": "JesseTheRobot",
  "license": "MIT",
  "scripts": {
    "dev:build": "rm -rf ./cjs && rm -rf ./esm && tsc -p ./cjs.json && ttsc -p ./esm.json",
    "build": "yarn run dev:build"
  },
  "exports": {
    ".": {
      "import": "./esm/node/index.js",
      "require": "./cjs/node/index.js"
    },
    "./*": {
      "import": "./*.js",
      "require": "./*.js"
    }
  },
  "files": [
    "cjs/**/*",
    "esm/**/*"
  ],
  "peerDependencies": {
    "@bundlr-network/client": "^0.6.2"
  },
  "dependencies": {
    "@noble/ed25519": "^1.6.0",
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@bundlr-network/client": "^0.6.2",
    "@noble/ed25519": "^1.6.0",
    "axios": "^0.26.1"
  }
}