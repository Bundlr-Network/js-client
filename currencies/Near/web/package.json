{
    "name": "@bundlr-network/near-web",
    "version": "0.0.1",
    "description": "Near Currency for Bundlr",
    "main": "index.js",
    "author": "JesseTheRobot",
    "license": "MIT",
    "scripts": {
        "dev:build": "rm -rf ./cjs && rm -rf ./esm && tsc -p ./cjs.json && ttsc -p ./esm.json",
        "build": "yarn run dev:build && webpack"
    },
    "exports": {
        ".": {
            "import": "./esm/web/index.js",
            "require": "./cjs/web/index.js"
        },
        "./*": {
            "import": "./*.js",
            "require": "./*.js"
        }
    },
    "files": [
        "cjs/**/*",
        "esm/**/*"
    ],
    "peerDependencies": {
        "@bundlr-network/client": "^0.8.0-rc1"
    },
    "dependencies": {
        "@noble/ed25519": "^1.6.0",
        "bignumber.js": "^9.0.2",
        "bn.js": "^5.2.0",
        "bs58": "^5.0.0",
        "js-sha256": "^0.9.0",
        "near-api-js": "^0.44.2"
    },
    "devDependencies": {
        "@bundlr-network/client": "^0.8.0-rc1",
        "@noble/ed25519": "^1.6.0",
        "bignumber.js": "^9.0.2",
        "bn.js": "^5.2.0",
        "bs58": "^5.0.0",
        "inspectpack": "^4.7.1",
        "js-sha256": "^0.9.0",
        "near-api-js": "^0.44.2",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2"
    }
}